{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "composite": false,
    "noEmit": true,
    "outDir": "lib",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": []
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
